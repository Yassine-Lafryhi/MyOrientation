<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F8F8F8">

    <ImageView
        android:layout_marginTop="8dp"
        android:id="@+id/logo"
        android:layout_width="140dp"
        android:layout_height="140dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:src="@drawable/logo_transparent"
        />

    <RelativeLayout
        android:padding="6dp"
        android:elevation="8dp"
        android:id="@+id/Menu1"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/logo"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="20dp"
        android:layout_alignParentLeft= "true"
        android:background="@drawable/start">

        <ImageView
            android:id="@+id/img_menu1"
            android:layout_width="120dp"
            android:layout_centerHorizontal="true"
            android:layout_height="120dp"
            android:src="@drawable/presentation_team"/>
        <TextView
            android:layout_below="@id/img_menu1"
            android:layout_width="320dp"
            android:layout_height="50dp"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="6dp"
            android:elevation="4dp"
            android:enabled="false"
            android:textStyle="bold"
            android:gravity="center"
            android:text="Students \nStatistics"
            android:textAllCaps="false"
            android:textColor="#000000"
            android:textSize="22dp" />




    </RelativeLayout>



    <RelativeLayout
        android:padding="6dp"
        android:elevation="8dp"
        android:id="@+id/Menu2"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/logo"
        android:layout_marginRight="14dp"
        android:layout_alignParentRight="true"
        android:layout_marginTop="20dp"
        android:background="@drawable/start">

        <ImageView
            android:id="@+id/img_menu2"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_centerHorizontal="true"
            android:src="@drawable/personal_data_1" />
        <TextView
            android:layout_below="@id/img_menu2"
            android:layout_width="320dp"
            android:layout_height="50dp"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="6dp"
            android:elevation="4dp"
            android:enabled="false"
            android:textStyle="bold"
            android:gravity="center"
            android:text="Students \nList"
            android:textAllCaps="false"
            android:textColor="#000000"
            android:textSize="22dp" />




    </RelativeLayout>


    <RelativeLayout
        android:padding="6dp"
        android:elevation="8dp"
        android:id="@+id/Menu3"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/Menu1"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="10dp"
        android:layout_alignParentLeft= "true"
        android:background="@drawable/start">

        <ImageView
            android:id="@+id/img_menu3"
            android:layout_width="120dp"
            android:layout_centerHorizontal="true"
            android:layout_height="120dp"
            android:src="@drawable/complete_task_1"/>

        <TextView
            android:layout_width="320dp"
            android:layout_height="50dp"
            android:layout_below="@id/img_menu3"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="6dp"
            android:elevation="4dp"
            android:enabled="false"
            android:gravity="center"
            android:text="Principal \nList"
            android:textAllCaps="false"
            android:textColor="#000000"
            android:textSize="22dp"
            android:textStyle="bold" />




    </RelativeLayout>



    <RelativeLayout
        android:padding="6dp"
        android:elevation="8dp"
        android:id="@+id/Menu4"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/Menu2"
        android:layout_marginRight="14dp"
        android:layout_alignParentRight="true"
        android:layout_marginTop="10dp"
        android:background="@drawable/start">

        <ImageView
            android:id="@+id/img_menu4"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_centerHorizontal="true"
            android:src="@drawable/profile_analyze" />

        <TextView
            android:layout_width="320dp"
            android:layout_height="50dp"
            android:layout_below="@id/img_menu4"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="6dp"
            android:elevation="4dp"
            android:enabled="false"
            android:gravity="center"
            android:text="Waiting \nList"
            android:textAllCaps="false"
            android:textColor="#000000"
            android:textSize="22dp"
            android:textStyle="bold" />
    </RelativeLayout>



    <Button
        android:id="@+id/LogIn"
        android:layout_width="320dp"
        android:layout_height="50dp"
        android:layout_below="@id/Menu4"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/logout_button"
        android:elevation="4dp"
        android:text="Log Out"
        android:layout_centerHorizontal="true"
        android:textAllCaps="false"
        android:textColor="#fff"
        android:textSize="26dp" />




</RelativeLayout>